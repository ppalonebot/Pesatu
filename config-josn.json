{
  "sfu": {
    "ballast": 0,
    "withstats": false
  },
  "router": {
    "maxbandwidth": 1500,
    "maxpackettrack": 500,
    "audiolevelthreshold": 40,
    "audiolevelinterval": 1000,
    "audiolevelfilter": 20,
    "simulcast": {
      "bestqualityfirst": true,
      "enabletemporallayer": false
    }
  },
  "webrtc": {
    "portrange": [5000, 5200],
    "sdpsemantics": "unified-plan",
    "mdns": true,
    "candidates": {},
    "timeouts": {
      "disconnected": 5,
      "failed": 25,
      "keepalive": 2
    },
    "iceserver": [
      {
        "urls": ["stun:stun.stunprotocol.org:3478"]
      },
      {
        "urls": ["turn:turn.awsome.org:3478"],
        "username": "awsome",
        "credential": "awsome"
      }
    ]
  },
  "turn": {
    "enabled": false,
    "realm": "ion",
    "address": "0.0.0.0:3478",
    "auth": {
      "credentials": "pion=ion,pion2=ion2"
    }
  },
  "log": {
    "v": 2
  }
}
